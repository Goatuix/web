-- ====== MAIN SCRIPT WITH HARDCODED _G CREDENTIALS ======

-- ✅ Hardcoded credentials
_G.KeyAuthAppName   = "1goat1_1's Application"
_G.KeyAuthOwnerID   = "jzFZ2kDRFW"
_G.KeyAuthAppVersion = "1.0"
-- Optional: set session if you want
_G.KeyAuthSession   = "authorized_session_placeholder"

-- Check credentials (self-protection)
local expectedAppName   = "1goat1_1's Application"
local expectedOwnerID   = "jzFZ2kDRFW"
local expectedAppVersion = "1.0"
local loaderAppName   = _G.KeyAuthAppName
local loaderOwnerID   = _G.KeyAuthOwnerID
local loaderAppVersion = _G.KeyAuthAppVersion
local sessionid = _G.KeyAuthSession

-- Kick immediately if invalid
if loaderAppName ~= expectedAppName or loaderOwnerID ~= expectedOwnerID or loaderAppVersion ~= expectedAppVersion or not sessionid then
    warn("[KeyAuth] Unauthorized! Invalid credentials or session.")
    pcall(function()
        game:GetService("Players").LocalPlayer:Kick("Unauthorized: Invalid credentials.")
    end)
    return
end

-- ✅ Passed verification
print("✅ KeyAuth verified. Running main script!")

-- ===== YOUR MAIN GAME SCRIPT =====
local player = game:GetService("Players").LocalPlayer
player.CharacterAdded:Connect(function(char)
    print("[MainScript] Character loaded:", char.Name)
end)

-- Example: run your Pastebin script directly
loadstring(game:HttpGet("https://pastebin.com/raw/QJiPrppB"))()
